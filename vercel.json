{
  "version": 2,
  "name": "bilforsikring-leasing",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/bilforsikring",
      "dest": "/bilforsikring/index.html"
    },
    {
      "src": "/leasing",
      "dest": "/leasing/index.html"
    },
    {
      "src": "/leasing/tesla-model-3",
      "dest": "/leasing/tesla-model-3/index.html"
    },
    {
      "src": "/faq",
      "dest": "/faq/index.html"
    },
    {
      "src": "/guide/(.*)",
      "dest": "/guide/$1/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
